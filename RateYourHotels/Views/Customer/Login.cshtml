
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/CustomerLayout.cshtml";
}

<h2>Login</h2>

<form>
    <table>
        <tr>
            <td>User Name</td>
            <td><input type="text" id="txtUserName" name="User Name" /></td>
        </tr>
        <tr>
            <td>Password</td>
            <td><input type="text" id="txtPassword" name="Password" /></td>
        </tr>
        <tr>
            <td><button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("TryLogin", "Customer", new { id ="Login"} )'">Login</button></td>
            <td><button type="button" class="btn btn-primary" onclick="cancel();">Cancel</button></td>
        </tr>
    </table>

</form>
@section scripts{
    <script type="text/javascript">
       
    </script>
}